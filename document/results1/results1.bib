	Doi = {10.1073/pnas.0606843103},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Animals, Protein Structure: Tertiary, Models: Molecular, Thermodynamics, Protein Structure: Secondary, Hydrogen Bonding, Protein Denaturation, Humans, Protein Folding},
	Language = {eng},
