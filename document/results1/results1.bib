	Journal = {Biochemistry},
	Keywords = {Protein Structure: Quaternary, Nitrogen Isotopes, Amyloid beta-Peptides, Peptide Fragments, Protein Binding, Protein Structure: Secondary, Carbon Isotopes, Alzheimer Disease, Molecular Sequence Data, Models: Chemical, Amino Acid Sequence, Humans, Magnetic Resonance Spectroscopy, Salts, Models: Molecular},
