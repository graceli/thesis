	Journal = {Proteins},
	Keywords = {Adsorption, Amyloid, Hydrophobic and Hydrophilic Interactions, Peptides, Protein Conformation, Water, Molecular Dynamics Simulation, Protein Structure: Secondary, Hydrogen Bonding, Octanes, Protein Folding},
	Language = {eng},
