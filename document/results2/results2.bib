	Pmid = {8457674},
	Rating = {0},
	Read = {Yes},
	Title = {Structure of $\beta$-Crystallite Assemblies Formed by Alzheimer Beta-Amyloid Protein Analogues: Analysis by X-Ray Diffraction},
	Uri = {\url{papers2://publication/doi/10.1016/S0006-3495(93)81393-6}},
