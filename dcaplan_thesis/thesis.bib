	Doi = {10.1021/bi100749p},
	Journal = {Biochemistry},
	Keywords = {Point Mutation, Electron Transport Complex IV, Oxidation-Reduction, Carbon Monoxide, Oxygen, Threonine, Models: Molecular, Protons, Thermus thermophilus},
	Language = {eng},
	Month = {Aug},
	Number = {33},
	Pages = {7033--9},
	Pmid = {20677778},
	Rating = {0},
	Title = {Functional role of Thr-312 and Thr-315 in the proton-transfer pathway in ba3 Cytochrome c oxidase from Thermus thermophilus},
