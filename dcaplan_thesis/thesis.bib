	Journal = {Biochim Biophys Acta},
	Keywords = {Oxidoreductases, Oxidation-Reduction, Electron Transport Complex IV, Models, Biological, Copper, Proton Pumps, Rhodobacter sphaeroides, Heme},
	Local-Url = {file://localhost/Users/david/Documents/Papers/Brzezinski/2003/Biochim%20Biophys%20Acta%202003%20Brzezinski.pdf},
