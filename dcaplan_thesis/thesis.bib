	Journal = {Journal of computational chemistry},
	Keywords = {Static Electricity, Water, Protein Structure: Secondary, Models: Molecular, Proteins, Computer Simulation, Solvents, Computational Biology},
	Language = {eng},
	Local-Url = {file://localhost/Users/david/Documents/Papers/Patel/2004/Journal%20of%20computational%20chemistry%202004%20Patel-1.pdf},
