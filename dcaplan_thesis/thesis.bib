	Date-Modified = {2011-03-29 19:22:56 -0400},
	Journal = {Biochemistry},
	Keywords = {Manganese, Molecular Sequence Data, Protons, Copper, Proton Pumps, Protein Binding, Hydrogen Bonding, Binding Sites, Ligands, Animals, Amino Acid Sequence, Models, Molecular, Water, Electron Transport Complex IV, Magnesium, Electron Spin Resonance Spectroscopy, Cyanides, Rhodobacter sphaeroides, Oxidation-Reduction, Cattle},
