	Local-Url = {file://localhost/Users/david/Documents/Papers/Br%C3%A4nd%C3%A9n/2006/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202006%20Br%C3%A4nd%C3%A9n.pdf},
