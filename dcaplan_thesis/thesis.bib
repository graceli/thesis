	Language = {eng},
	Local-Url = {file://localhost/Users/david/Documents/Papers/Muramoto/2007/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%20of%20the%20United%20States%20of%20America%202007%20Muramoto.pdf},
